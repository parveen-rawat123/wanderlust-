<% layout("/layouts/boilerplate")%>
  <h3>All Listing</h3>
  <div class="row  row-cols-lg-3 row-cols-md-2 row-cols-sm-1
  row-cols-xs-1">
    <% for(let listing of allisting){  %>
      <div class="card col" >
        <img src="<%= listing.image %>" class="card-img-top" alt="listing-image"
        style="height: 21rem;">
        <div class="card-body">
          <p class="card-text">
           <b> <%= listing.title %> </b><br>
            &#8377;<%= listing.price.toLocaleString("en-IN") %> /night  </p>
          </div>
        </div>
        <%  } %>
      </div> 
